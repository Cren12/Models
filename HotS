# Valeera

mult <- .04
basic.attack <- 79 * 1.3
n.basic.attack <- 4
garrote <- 160 * 2
blade.flurry <- 130
sinister.strike <- 110 * 2.25 * 1.5
eviscerate <- 255

(n.basic.attack * basic.attack + garrote + blade.flurry + sinister.strike + eviscerate) * (1 + mult) ^ 19
# 3132.99

# Nova

mult <- .04
basic.attack <- 109 * 1.3 * 1.6 * 1.2
n.basic.attack <- 3
snipe <- 230 * 1.2
pinning.shot <- 100 * 2

(n.basic.attack * basic.attack + snipe + pinning.shot) * (1 + mult) ^ 19
# 2722.454
